class Solution {
public:
    int findKthLargest(vector<int>& A, int B) {
        priority_queue<int,vector<int>,greater<int>> maxh;
        for(int i=0;i<A.size();i++)
        {
            maxh.push(A[i]);
            if(maxh.size()>B)
            maxh.pop();
        }
        return maxh.top();
    }
};
