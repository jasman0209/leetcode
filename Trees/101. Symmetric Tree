class Solution {
public:
    bool checksym(TreeNode* a,TreeNode *b)
    {
       if(a==NULL && b==NULL)
           return true;
        if(a!=NULL&&b!=NULL)
        {
            return (a->val==b->val&& checksym(a->left,b->right)&&checksym(a->right,b->left));
        }
        else
            return false;
    }
    bool isSymmetric(TreeNode* root) {
        if(root==NULL) return true;
        return checksym(root->left,root->right);
        
    }
};
