class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> v;
        if(numRows==0) return {};
        vector<int> v1;
        v1.push_back(1);
        v.push_back(v1);
        if(numRows==1) return v;
        v1.push_back(1);
        v.push_back(v1);
        if(numRows==2) return v;
        
        for(int i=3;i<=numRows;i++)
        {
            vector<int> v2;
            v2.push_back(1);
            for(int j=1;j<v[i-2].size();j++)
            {
                v2.push_back(v[i-2][j]+v[i-2][j-1]);
            }
            v2.push_back(1);
            v.push_back(v2);
        }
        return v;
        
    }
};
