class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n)
{
    // num1r, num2r read iterators on nums1 and num2r, wi - write iterator
	for (int num1r = m - 1, num2r = n - 1, wi = m + n - 1;
		num2r >= 0; --wi)
	{
		if (num1r >= 0 && nums1[num1r] > nums2[num2r])
		{
			nums1[wi] = nums1[num1r--];
		}
		else
		{
			nums1[wi] = nums2[num2r--];
		}
	}
}
};
