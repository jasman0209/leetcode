class Solution {
public:
    vector<int> countBits(int num) {
        int i=0,b=1;
        vector<int> v(num+1,0);
        while(b<=num)
        {
            while(i<b&&i+b<=num)
            {
                v[i+b]=v[i]+1;
                i++;
            }
            //cout<<b<<endl;
           b= b<<1;
            i=0;
        }
        return v;
    }
};
